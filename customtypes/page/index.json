{
    "id": "page",
    "label": "Page",
    "format": "page",
    "repeatable": true,
    "status": true,
    "json": {
        "Main": {
            "uid": {
                "type": "UID",
                "config": {
                    "label": "UID",
                    "placeholder": ""
                }
            },
            "title": {
                "type": "StructuredText",
                "config": {
                    "label": "Title",
                    "placeholder": "",
                    "allowTargetBlank": true,
                    "single": "heading1"
                }
            },
            "parent": {
                "type": "Link",
                "config": {
                    "label": "Parent",
                    "select": "document",
                    "customtypes": ["page"]
                }
            },
            "slices": {
                "type": "Slices",
                "fieldset": "Slice Zone",
                "config": {
                    "choices": {
                        "image_ticker": {
                            "type": "SharedSlice"
                        },
                        "cta": {
                            "type": "SharedSlice"
                        },
                        "feature_grid": {
                            "type": "SharedSlice"
                        },
                        "newsletter_embed": {
                            "type": "SharedSlice"
                        },
                        "gallery": {
                            "type": "SharedSlice"
                        },
                        "image_full": {
                            "type": "SharedSlice"
                        },
                        "callout": {
                            "type": "SharedSlice"
                        },
                        "hero": {
                            "type": "SharedSlice"
                        },
                        "quote": {
                            "type": "SharedSlice"
                        },
                        "text": {
                            "type": "SharedSlice"
                        },
                        "image": {
                            "type": "SharedSlice"
                        },
                        "image_cards": {
                            "type": "SharedSlice"
                        },
                        "text_with_image": {
                            "type": "SharedSlice"
                        }
                    }
                }
            }
        },
        "SEO": {
            "meta_title": {
                "type": "Text",
                "config": {
                    "label": "Meta Title",
                    "placeholder": "A title of the page used for social media and search engines"
                }
            },
            "meta_description": {
                "type": "Text",
                "config": {
                    "label": "Meta Description",
                    "placeholder": "A brief summary of the page"
                }
            },
            "meta_image": {
                "type": "Image",
                "config": {
                    "label": "meta_image",
                    "constraint": {},
                    "thumbnails": []
                }
            },
            "og_image_override": {
                "type": "Image",
                "config": {
                    "label": "og_image_override",
                    "constraint": {
                        "width": 2400,
                        "height": 1260
                    },
                    "thumbnails": []
                }
            },
            "noindex": {
                "type": "Boolean",
                "config": {
                    "label": "noindex",
                    "placeholder_false": "false",
                    "placeholder_true": "true",
                    "default_value": false
                }
            }
        }
    }
}
